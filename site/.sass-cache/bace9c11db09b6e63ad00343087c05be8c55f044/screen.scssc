@import "compass";
@import "fonts";
@import "compass/css3/box-sizing";

$blueColor: #c7e5df;
$blackColor: #000000;
$brownishYellowColor: #f4eac6;
$darkBlueColor: #558178;
$darkerBlueColor: #34524c;
$redColor: #e95c54;
$border:4px solid $blackColor;

body{
  font-size:62.5%;
  font-family: corki, arial, sans-serif;
  background-color: $brownishYellowColor;
}

#container{
  position: relative;
}

h1{
  max-width: 100%;
  height: auto !important;
  a{
    @include box-sizing("border-box");
    background: url("../images/title_enroute.png")no-repeat;
    width: 262px;
    height: 170px;
    position: fixed;
    top: 20px;
    left: 50%;
    margin-left: -131px;

    z-index: 50;
    span{
      visibility: hidden;
    }
  }
}

@keyframes scaleanimation
{
  from
  {
    -webkit-transform: scale(1);
    -ms-transform: scale(1);
    -moz-transform: scale(1);
    -o-transform: scale(1);
  }
  to
  {
      -webkit-transform: scale(1.02);
      -ms-transform: scale(1.02);
      -moz-transform: scale(1.02);
      -o-transform: scale(1.02);
  }
}

@-webkit-keyframes scaleanimation
{
    from
     {
       -webkit-transform: scale(1);
       -ms-transform: scale(1);
       -moz-transform: scale(1);
       -o-transform: scale(1);
     }
     to
     {
         -webkit-transform: scale(1.02);
         -ms-transform: scale(1.02);
         -moz-transform: scale(1.02);
         -o-transform: scale(1.02);
     }
}

h2 span, h3 span{
