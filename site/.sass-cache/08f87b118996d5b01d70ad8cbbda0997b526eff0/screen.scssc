@import "compass";
@import "fonts";
@import "compass/css3/box-sizing";

$blueColor: #c7e5df;
$blackColor: #000000;
$brownishYellowColor: #f4eac6;
$darkBlueColor: #558178;
$darkerBlueColor: #34524c;
$redColor: #e95c54;
$border:4px solid $blackColor;

body{
  font-size:62.5%;
  font-family: corki, arial, sans-serif;
  background-color: $brownishYellowColor;
}

// --- fonts - typo --- //

h1{
  max-width: 100%;
  height: auto !important;
  a{
    @include box-sizing("border-box");
    background: url("../images/title_enroute.png")no-repeat;
    width: 262px;
    height: 170px;
    position: fixed;
    top: 20px;
    left: 50%;
    margin-left: -131px;

    z-index: 50;
    span{
      visibility: hidden;
    }
  }
}

h2 span, h3 span{
  display: none;
}

// --- fonts - typo --- //

// --- general elements --- //

a{
  text-decoration: none;
}

.left{
  float: left;
}

.right{
  float: right;
}

.clear{
  clear: both;
}

// --- general elements --- //

// --- styled content --- //

#container{
  position: relative;
}

#headBorder{
  width: 100%;
  height: 100px;
  position: fixed;
  background-color: $blueColor;
  border-bottom: $border;
  z-index: 40;
}

nav{
  position: absolute;
  top: -2px;
  width: 100%;
  height: 100px;
  z-index: 60;
  ul{
    position: fixed;
    float: left;
    z-index: 60;
    margin: 35px 0px 0px 50px;
    .current a{
      color: $blackColor;
    }
    li{
      margin-right: 30px;
      padding: 0;
      list-style: none;
      display: inline-block;
      a{
        padding: 3px 12px;
       	text-decoration: none;
       	color: $redColor;
        font-family: bebas-neue;
        font-size: 2.2em;
       	line-height: 100%;
        &:hover{ color: $blackColor; }
      }
    }
  }
  form{
    float: right;
  }
}
.active{ color: $blackColor; }

.introTextBg{
  @include box-sizing("border-box");
  background: url("../images/bg_intro.png");
  width: 100%;
  height: 300px;
  position: fixed;
  top: 0px;
  left: 0px;
  z-index: 20;
  text-align: center;
  p{
    padding-top: 95px;
