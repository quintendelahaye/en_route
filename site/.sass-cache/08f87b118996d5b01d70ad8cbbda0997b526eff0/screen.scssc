#vintageContainer{
  @include box-sizing("border-box");
  width: 700px;
  margin: 30px auto 20px auto;
  position: relative;
  .bigFrame{
    background: url("../images/5_vintage/frame_big.png")no-repeat;
    width: 286px;
    height: 285px;
    position: absolute;
    top: -20px;
    left: 210px;
    img{
      margin-top: 44px;
      margin-left: 45px;

      width: 198px;
      height: 199px;

      background-clip: border-box;
      -moz-background-clip: border-box;
      -webkit-background-clip: border-box;
      -o-background-clip: border-box;
    }
    &:hover{
      -webkit-transform: rotate(10deg);
      -moz-transform: rotate(10deg);
      -o-transform: rotate(10deg);
      -ms-transform: rotate(10deg);
      transform: rotate(10deg);
    }
  }
  #frameLeft, #frameRight{
    background: url("../images/5_vintage/frame_small.png")no-repeat;
    width: 186px;
    height: 185px;

    img{
      margin-top: 28px;
      margin-left: 28px;
      width: 131px;
      height: 130px;

      background-clip: border-box;
      -moz-background-clip: border-box;
      -webkit-background-clip: border-box;
      -o-background-clip: border-box;
    }
    &:hover{
      -webkit-transform: rotate(-10deg);
      -moz-transform: rotate(-10deg);
      -o-transform: rotate(-10deg);
      -ms-transform: rotate(-10deg);
      transform: rotate(-10deg);
    }
  }

  .frame{
    background: url("../images/5_vintage/frame_list.png")no-repeat;
    width: 151px;
    height: 150px;
    display: inline-block;
    margin-bottom: 20px;
    margin-left: 20px;
    img{
      margin-top: 22px;
      margin-left: 22px;
      width: 107px;
      height: 106px;

      background-clip: border-box;
      -moz-background-clip: border-box;
      -webkit-background-clip: border-box;
      -o-background-clip: border-box;
    }
    &:hover{
      -webkit-transform: rotate(-10deg) scaleX(1.1) scaleY(1.1); /* Chrome 4+, Op 15+, Saf 3.1, iOS Saf 3.2+ */
      -moz-transform: rotate(-10deg) scaleX(1.1) scaleY(1.1); /* Fx 3.5-15 */
      -ms-transform: rotate(-10deg) scaleX(1.1) scaleY(1.1); /* IE 9 */
      -o-transform: rotate(-10deg) scaleX(1.1) scaleY(1.1); /* Op 10.5-12 */
      transform: rotate(-10deg) scaleX(1.1) scaleY(1.1);
    }
  }
}

