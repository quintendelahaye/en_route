      background: url("../images/btn_teams.png")no-repeat 0px 0px;
      width: 148px;
      height: 26px;
      display: block;
      color: $darkBlueColor;
     &:hover{
       background: url("../images/btn_teams.png")no-repeat 0px -32px;
       color: white;
     }
    }
  }
}

#members{
  margin-left: 80px;
  margin-top: 10px;
  li{
    font-family: bebas-neue;
    font-size: 1.8em;
    color: $darkerBlueColor;
  }
}

.notReady{
  text-align: center;
  color: $darkBlueColor;
  font-family: bebas-neue;
  font-size: 3em;
}

iframe{
    margin: 20px auto 50px auto;
    width: 800px;
    height: 451px;
}


// --- forms --- //

.searchField{
  @include box-sizing("border-box");
  position: fixed;
  right: 0px;
  margin: 33px 100px 0px 0px;

  ol{
    background-color: #f6f6f8;
    li{
      padding: 10px;
      a{
        color: $redColor;
        font-size: 1.4em;
        font-family: bebas-neue;
        &:hover{
          color: black;
        }
      }
    }
  }
    form{
      .search {
        background: url('../images/icon_search.png') no-repeat;
          width: 20px;
          height: 21px;
          display: block;
          position: absolute;
          top: 8px;
          left: 7px;
          span{
            visibility: hidden;
          }
      }
      position: relative;
      input[type=text]{
      	background-color:white;
        border: none;
        width:150px;
        height:20px;
        padding:6px 15px 8px 35px;
      }
      #btnSubmitZoek{
        background: url("../images/btn_search.png")no-repeat 0px 0px;
        width: 10px;
        height: 10px;
        display: block;
        border: none;
        text-indent: -99999px;
        position: absolute;
        right: 8px;
        top: 13px;
        &:hover{
          background: url("../images/btn_search.png")no-repeat 0px -10px;
        }
      }
    }
}

#subscribe{
  margin-top: 20px;
  width: 500px;
  .left, .right{
    width: 232px;
  }
  legend{visibility: hidden; }
  input[type="text"], input[type="date"], input[type="email"]{
    width: 223px;
    height: 30px;
    border: $border;
  }
  label{
    font-family: bebas-neue;
    color: $darkerBlueColor;
    font-size: 1.6em;
    margin-left: 10px;
  }
  div{
    margin-bottom: 20px;
  }
  .error{
    background-color: $redColor;
  }
  .dateError{
    margin-top: 5px;
    text-align: center;
    p{
      font-size: 1.6em;
      font-family: bebas-neue;
      color: $redColor;
    }
  }
}

#booked{
  width: 700px;
  margin: 20px auto 20px auto;
  li{
    display: inline-block;
    color: $redColor;
    font-size: 2em;
  }
}

// --- forms --- //


@keyframes overslideintop{
  from{
    height: 0;
  }
  to{
    height: 56px;
  }
}

@-webkit-keyframes overslideintop{
    from{
       height: 0;
     }
     to{
         height: 56px;
     }
}

@keyframes fadein{
  from{
    opacity: 0;
  }
  to{
    opacity: 1;
  }
}

@-webkit-keyframes fadein{
    from{
      opacity: 0;
     }
     to{
       opacity: 1;
     }
}

@keyframes overslideinbottom{
  from{
    height: 0;
  }
  to{
    height: 28px;
  }
}

@-webkit-keyframes overslideinbottom{
  from{
     height: 0;
   }
   to{
       height: 28px;
   }
}

@keyframes slideintop{
  from{
    height: 0;
  }
  to{
    height: 130px;
  }
}

@-webkit-keyframes slideintop{
    from{
       height: 0;
     }
     to{
         height: 130px;
     }
}

@keyframes fadein{
  from{
    opacity: 0;
  }
  to{
    opacity: 1;
  }
}

@-webkit-keyframes fadein{
    from{
      opacity: 0;
     }
     to{
       opacity: 1;
     }
}

@keyframes slideinbottom{
  from{
    height: 0;
  }
  to{
    height: 39px;
  }
}

@-webkit-keyframes slideinbottom{
  from{
     height: 0;
   }
   to{
       height: 39px;
   }
}

@keyframes speakerAnimation{
  from{
    opacity: 0;
    width: 34px;
  }
  to{
    opacity: 1;
    width: 68px;
  }
}

@-webkit-keyframes speakerAnimation{
    from{
      opacity: 0;
      width: 34px;
     }
     to{
       opacity: 1;
       width: 68px;
     }
}

// --- buttons --- //

.btnMore{
  background: url("../images/btn_blue.png")no-repeat 0px 0px;
  width: 232px;
  height: 39px;
  display: block;
