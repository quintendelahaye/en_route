    form{
      .search {
        background: url('../images/icon_search.png') no-repeat;
          width: 20px;
          height: 21px;
          display: block;
          position: absolute;
          top: 8px;
          left: 7px;
          span{
            visibility: hidden;
          }
      }
      position: relative;
      input[type=text]{
      	background-color:white;
        border: none;
        width:150px;
        height:20px;
        padding:6px 15px 8px 35px;
      }
      #btnSubmitZoek{
        background: url("../images/btn_search.png")no-repeat 0px 0px;
        width: 10px;
        height: 10px;
        display: block;
        border: none;
        text-indent: -99999px;
        position: absolute;
        right: 8px;
        top: 13px;
        &:hover{
          background: url("../images/btn_search.png")no-repeat 0px -10px;
        }
      }
    }
}

#missionNav{
  @include box-sizing("border-box");
    margin: 50px auto 20px auto;
    width: 650px;

  #leftNav{
    #allMissions{
      background: url("../images/btn_all.png")no-repeat 0px 0px;
      width: 148px;
      height: 39px;
      display: block;
      &:hover, #allMissionsActive{
        background: url("../images/btn_all.png")no-repeat 0px -39px;
      }
      span{
        visibility: hidden;
      }
    }
    select{
      border: none;
      background-color: white;
      width: 148px;
      height: 30px;
      margin-top: 10px;
      font-family: bebas-neue;
      color: $darkBlueColor;
      font-size: 1.9em;
    }
  }
  #rightNav{
    width: 500px;
    ul{
      li{
        display: inline-block;
        margin: 5px;
          span{
            visibility: hidden;
          }
      }
      #mission1, #mission2, #mission3, #mission4, #mission5, #mission6{
        width: 148px;
        height: 32px;
        display: block;
      }
      #mission1{
        background: url("../images/btn_opdracht1.png")no-repeat 0px 0px;
        &:hover{background: url("../images/btn_opdracht1.png")no-repeat 0px -32px;}
      }
      #mission2{
        background: url("../images/btn_opdracht2.png")no-repeat 0px 0px;
        &:hover{background: url("../images/btn_opdracht2.png")no-repeat 0px -32px;}
      }
      #mission3{
        background: url("../images/btn_opdracht3.png")no-repeat 0px 0px;
        &:hover{background: url("../images/btn_opdracht3.png")no-repeat 0px -32px;}
      }
      #mission4{
        background: url("../images/btn_opdracht4.png")no-repeat 0px 0px;
        &:hover{background: url("../images/btn_opdracht4.png")no-repeat 0px -32px;}
      }
      #mission5{
        background: url("../images/btn_opdracht5.png")no-repeat 0px 0px;
        &:hover{background: url("../images/btn_opdracht5.png")no-repeat 0px -32px;}
      }
      #mission6{
        background: url("../images/btn_opdracht6.png")no-repeat 0px 0px;
        &:hover{background: url("../images/btn_opdracht6.png")no-repeat 0px -32px;}
      }
    }
  }
}

#allMissionsActive{ background: url("../images/btn_all.png")no-repeat 0px -39px; width: 148px; height: 39px; display: block; span{visibility: hidden;}}

.activeMission1, .activeMission2, .activeMission3, .activeMission4, .activeMission5, .activeMission6{ display: block; width: 148px; height: 32px; }
.activeMission1{background: url("../images/btn_opdracht1.png")no-repeat 0px -32px;}
.activeMission2{background: url("../images/btn_opdracht2.png")no-repeat 0px -32px;}
.activeMission3{background: url("../images/btn_opdracht3.png")no-repeat 0px -32px;}
.activeMission4{background: url("../images/btn_opdracht4.png")no-repeat 0px -32px;}
.activeMission5{background: url("../images/btn_opdracht5.png")no-repeat 0px -32px;}
.activeMission6{background: url("../images/btn_opdracht6.png")no-repeat 0px -32px;}

//----------- media ---------------------------------------------------------------------------------------------------------

@media  only screen and (max-width: 1300px) {
  nav ul li a{
    font-size: 1.8em;
  }
}

@media  only screen and (max-width: 1170px) {
  nav{
    ul{
      li{
        margin-right: 5px;
        a{
          font-size: 1.7em;
        }
      }
    }
  }
}

//ipad
@media  only screen and (max-width: 1024px) {
  .searchField{
  margin: 33px 20px 0px 0px;
    form{
      input[type=text]{
        width:100px;
        height:20px;
        font-size: 0.9em;
      }
    }
  }
}


@media  only screen and (max-width: 980px) {

  h1{
    a{
      background-size: 90%;
      margin-left: -60px;
    }
  }
  .nav{
    position: fixed;
    height: 10px;
    ul{
      position: absolute;
      top: 0;
      left: 0;
      width: 250px;
      height: 25px;
      padding-top: 30px;
      background:url("../images/icon_menu.png") no-repeat 0px 0px;
      &:hover{
        li{
          display: block;
          text-align: left;
          a{
            background-color: white;
            font-size: 4em;
          }
        }
      }
      li{
        display: none; /* hide all <li> items */
        margin-top: 0;
      }
      li:first-child{
        margin-top: 14px;
      }
    }
    .right ul{
      left: auto;
       right: 0;
    }
    .center ul{
      left: 50%;
      margin-left: -90px;
    }
  }
}

@media  only screen and (max-width: 700px) {

  .worksContainer{
    width: 700px;

  }
}

@media only screen and (max-width: 640px) {
  .searchField{
    margin: 124px 20px 0px 0px;
      form{
        input[type=text]{
          width:80px;
          height:20px;
        }
      }
    }

  .worksContainer{
    width: 600px;
  }

  #missionNav{
    width: 550px;
    #rightNav{
      margin-top: 0px;
      width: 390px;
      ul{
        margin-left: 20px;
        li{
          width: 180px;
            margin: 0px 0px 15px 0px;
          }
      }
    }
  }
}

@media  only screen and (max-width: 400px) {
  h1{
    a{
      background-size: 60%;
      margin-top: 20px;
    }
  }

  .introTextBg{
   p{
     font-size: 1.8em;
   }
  }
  .worksContainer{
    width: 350px;
  }

  .explaination{
    width: 90%;
    margin-left: 20px;
  }

  .collageContainer{
    .collagePicContainer{
      padding: 0px 10px 10px 10px;
    }
    .overimgTop a, .overimgBottom a{
      left:14px;
    }
    .overimgBottom a{
      bottom: 16px;
    }
  }

  #dialectsContainer{
      width: 140px;
      font-size: 90%;

    .dialectContainer{
      margin-left: 30px;
      margin-bottom: 30px;
      .overimgBottom{
        font-size: 1.8em;
      }
    }
    .dateDialects{
      font-size: 1.3em;
    }
  }

  #fashionContainer{
    margin: 0;
    display: block;
    min-height: 250px;
    width: 140px;
    #dress{
      top: 180px;
      left: 220px;
    }
    #textContainer{
      margin-top: 40px;
      width: 120px;
      top: -70px;
      #labels{
        top: 80px;
        left:-55px;
      }
      #amountOfShops{
        width: 320px;
        top: 200px;
        left: 43px;
      }

      #shops{
        width: 320px;
        top: 250px;
        left: 25px;
      }
    }
  }

  #imageContainer{
    width: 350px;
    #statuePic{
      padding-left: 85px;
      img{
        /*width: 223px;
        height: 288px;*/
        width: 180px;
        height: 180px;
         border: $border;
       }
    }
    h4{
      width: 100px;
      font-size: 1.6em;
    }
    h4:first-of-type{
      left: 0px;
    }
    h4:last-of-type{
      text-align: right;
      color: $redColor;
      top: 150px;
      right: 0px;
      img{
        padding-right: 10px;
      }
    }
  }

  #vintageContainer{
    width: 370px;
    min-height: 600px;
    margin-bottom: -100px;
    .bigFrame{
      top: 165px;
      left: 30px;
    }
    #frameLeft, #frameRight{
      position: absolute;
    }
    #frameLeft{
        top: -20px;
        left: 0px;
    }
    #frameRight{
        top: 450px;
        right: 50px;
    }
  }

  #lostPicContainer{
    width: 300px;
    figure{
      img{
        margin-left: 68px;

        width: 208px;
        height: 147px;
      }
    }
    .lines{

      position: absolute;
      top: 17px;
      left: -50px;

      -webkit-transform: rotate(-30deg) scale(.6); /* Chrome 4+, Op 15+, Saf 3.1, iOS Saf 3.2+ */
      -moz-transform: rotate(-30deg) scale(.6); /* Fx 3.5-15 */
      -ms-transform: rotate(-30deg) scale(.6); /* IE 9 */
      -o-transform: rotate(-30deg) scale(.6); /* Op 10.5-12 */
      transform: rotate(-30deg) scale(.6); /* Fx 16+, IE 10+ */


      p{
        font-size: 2.7em;
        top: -20px;
      }

      h4{
        font-size: 3em;
        font-family: bebas-neue;
        top: 15px;
      }
    }
  }

  #topBtn{
    width: 300px;
    .btnTop{
      top: -25px;
      left: 130px;
    }
  }

  footer{
    width: 400px;
    #footerContent{
      width: 400px;
      h4{
        top: -32px;
        left: -3px;
      }
      ul{
        margin-left: 10px;
        li{
          font-size: 1.1em;
          margin-right: 10px;
        }

        .fbicon,.twtricon{
          top: -30px;
        }
      }
    }
  }

  .searchField{
  margin: 124px 20px 0px 0px;
    form{
      input[type=text]{
        width:50px;
        height:20px;
      }
    }
  }

  #missionNav{
    margin: 0px 0px 20px 30px;
    width: 370px;
    #rightNav{
      margin-top: 20px;
      width: 390px;
      ul{
        margin-left: 20px;
        li{
          width: 180px;
            margin: 5px 0px 15px 0px;
          }
      }
    }
  }

  #soundBoard{
    width: 360px;
     li{
       margin-top: 20px;
     }
  }

  #allShops{
    width: 350px;
    margin-top: 20px;
    margin-left: 10px;
    li{
      margin-bottom: 10px;
    }
  }

  #detailGroup{
    width: 400px;
    img{
      width: 350px;
      height: 350px;
    }
    p{
      width: 400px;
    }
  }

