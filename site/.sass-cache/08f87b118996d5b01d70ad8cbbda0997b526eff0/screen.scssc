  background-color: $brownishYellowColor;
}

// --- fonts - typo --- //

h1{
  max-width: 100%;
  height: auto !important;
  a{
    @include box-sizing("border-box");
    background: url("../images/title_enroute.png")no-repeat;
    width: 262px;
    height: 170px;
    position: fixed;
    top: 20px;
    left: 50%;
    margin-left: -131px;

    z-index: 50;
    span{
      visibility: hidden;
    }
  }
}

h2 span, h3 span{
  display: none;
}

// --- fonts - typo --- //

// --- general elements --- //

a{
  text-decoration: none;
}

.left{
  float: left;
}

.right{
  float: right;
}

.clear{
  clear: both;
}

// --- general elements --- //

// --- styled content --- //

#container{
  position: relative;
}

#headBorder{
  width: 100%;
  height: 100px;
  position: fixed;
  background-color: $blueColor;
  border-bottom: $border;
  z-index: 40;
}

nav{
  position: absolute;
  top: -2px;
  width: 100%;
  height: 100px;
  z-index: 60;
  ul{
    position: fixed;
    float: left;
    z-index: 60;
    margin: 35px 0px 0px 50px;
