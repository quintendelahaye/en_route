.collageContainer{
  @include box-sizing("border-box");
  margin-top: 20px;
  .collagePicContainer{
    display: inline-block;
    position: relative;
    margin-right: auto;
    margin-bottom: 10px;
    padding-left: 35px;
    z-index: 6;

    figure img{
      width: 145px;
      height: 145px;

      border:$border;
      background-clip: border-box;
      -moz-background-clip: border-box;
      -webkit-background-clip: border-box;
      -o-background-clip: border-box;
    }
    &:hover .overimgTop a ,&:hover .overimgBottom a{
      display: block;
    }
  }

  .overimgTop a, .overimgBottom a{
    color: white;
    display: none;
    position: absolute;
    width: 145px;
    opacity: 0.95;
    left:39px;
    text-align: center;

    animation-timing-function: ease-in;
    -moz-animation-timing-function: ease-in;
    -webkit-animation-timing-function: ease-in;

    p, .iconCalender{
      color: #ffffff;
      animation: fadein 0.5s;
      -moz-animation: fadein 0.5s;
      -webkit-animation: fadein 0.5s;
      -o-animation: fadein 0.5s;
      -ms-animation: fadein 0.5s;
    }
  }

  .overimgTop a{
    background: url("../images/1_collage/over_top.png");
    top: 4px;
    z-index: 7;
    padding-top: 30px;
    height: 56px;
    font-family: bebas-neue;
    font-size: 2em;

    animation: overslideintop 0.5s;
    -moz-animation: overslideintop 0.5s;
    -webkit-animation: overslideintop 0.5s;
    -o-animation: overslideintop 0.5s;
    -ms-animation: overslideintop 0.5s;
  }

  .overimgBottom a{
    background: url("../images/1_collage/over_bottom.png");
    bottom: 6px;
    z-index: 8;
    height: 28px;
    font-size: 1.6em;
    padding-top: 18px;

    animation: overslideinbottom 0.5s;
    -moz-animation: overslideinbottom 0.5s;
    -webkit-animation: overslideinbottom 0.5s;
    -o-animation: overslideinbottom 0.5s;
    -ms-animation: overslideinbottom 0.5s;
  }

  .iconCalender{
    position: absolute;
    bottom: 8px;
    left: 10px;
    background: url("../images/1_collage/icon_calender.png")no-repeat;
    width: 23px;
    height: 25px;
  }
}

@keyframes overslideintop{
  from{
    height: 0;
  }
  to{
    height: 56px;
  }
